<div>
  <h3>Already Registered? Sign In!</h3>
  <%= form_for :session, url: session_path do |form| %>
</div>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Sign In" %>
  </div>
<% end %>
